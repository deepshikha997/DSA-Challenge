class Solution
{
    //Function to sort an array using quick sort algorithm.
    static void quickSort(int arr[], int low, int high)
    {
        Arrays.sort(arr);
        int n=arr.length;
        int temp1 = arr[0];
        int temp2 = arr[n-1];
        partition(arr,temp1,temp2);
    }
    static int partition(int arr[], int low, int high)
    {
        // your code here
        int temp = 0;
        for(int i=0; i<high; i++){
            temp = arr[i];
            return temp;
        }
        return 0;
    } 
}
