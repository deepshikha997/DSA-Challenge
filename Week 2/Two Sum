class Solution {
    public int[] twoSum(int[] nums, int target) {
        int temp = nums.length;
        for(int i=0;i<temp-1;i++){
            for(int j=i+1;j<temp;j++){
                if(nums[i]+nums[j]==target){
                    return new int[]{i,j};
                }
            }
        }
        return new int[]{};
    }
}
