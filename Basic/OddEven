import java.util.*;
import java.lang.*;
import java.io.*;

class WorkAtTech
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int arr[] = new int[n];
		
		for(int i=0; i<n; i++){
			
			arr[i]= sc.nextInt();
			
			if(arr[i]%2==0){
				System.out.println("EVEN");
			}else{
				System.out.println("ODD");
			}
		}
		
	}
}