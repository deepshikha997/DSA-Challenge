import java.util.*;
import java.lang.*;
import java.io.*;

class WorkAtTech
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int candies = sc.nextInt();
		int cousins = sc.nextInt();
		
		if(candies%cousins==0){
			System.out.print("YES");
		}else{
			System.out.print("NO");
		}	
	}
}
