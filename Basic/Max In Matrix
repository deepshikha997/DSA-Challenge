import java.util.*;
import java.lang.*;
import java.io.*;

class WorkAtTech
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		
		int row = sc.nextInt();
		int col = sc.nextInt();
		int max= 0;
		
		int arr[][] = new int[row][col];
		for(int i=0; i<row; i++){
			for(int j=0; j<col; j++){
				int integer = sc.nextInt();
				if(max<integer){
					max=integer;
				}
			}
		}
		System.out.println(max);
	}
}