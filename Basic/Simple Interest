import java.util.*;
import java.lang.*;
import java.io.*;

class WorkAtTech
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		double p = sc.nextDouble();
		double r = sc.nextDouble();
		double t = sc.nextDouble();
		
		double SI;
		SI = (p*r*t)/100;
		System.out.println(String.format("%.6f",SI));
	}
}