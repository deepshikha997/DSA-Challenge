import java.util.*;
import java.lang.*;
import java.io.*;

class WorkAtTech
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		
		for(int i=0; i<T; i++){
			int n = sc.nextInt();
			if(n>7){
				System.out.println("UP");
			}else if(n<7){
				System.out.println("DOWN");
			}else{
				System.out.println("EQUAL");
			}
		}
		sc.close();
	}
}